import javax.swing.plaf.synth.SynthOptionPaneUI;

public class NestedLoopDemo {
	public static void main(String[] args) {
		int i = 1; // 초기화
		while(i <= 9) { // i 조건
			int dan = 2; // dan 초기화
			while(dan <= 9) { // dan 조건
				System.out.print(dan + " x " + i + " = " + dan*i + "\t");
				dan++; // dan 증감식
			}
			System.out.println();
			i++;
		}
		
		for(int a = 1; a <= 26; a++ ) {
			for(int j = 1; j <= a; j++) {
				System.out.println((char)(i));
			}
			System.out.println();
		}
		
	}
	
}
