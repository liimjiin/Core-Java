import java.util.Scanner;

public class DoDemo {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("ëª‡ : ");
		int su = sc.nextInt();
		int answer = 1;
		for(int i = su; i > 0; i--) {
			answer *= su;
		}
		System.out.println(su + "! = " + answer);
	}

}
