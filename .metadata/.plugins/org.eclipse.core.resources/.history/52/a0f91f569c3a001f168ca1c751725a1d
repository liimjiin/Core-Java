
public class LoopDemo {

	public static void main(String[] args) {
		// A ~ Z까지 출력하기 -> 시작과 끝이 명확하기 때문에 for문을 사용
		for(int i = 65 ; i <= 90 ; i++) {
			if(i%5 == 0) {
				System.out.println();
			}
			System.out.printf("%c\t", (char)i);
		}
		System.out.println();
	
	
	//1~100까지 4의 배수는 몇개?
	int count = 0;
	for(int f = 4 ; f <= 100 ; f++) {
		if(f%4 == 0) {
			count += 1;
					
			System.out.println(f);
			
			}
		}
	}
}


