import java.util.Scanner;

public class ScoreMgmt {
	public static void main(String[] args) {
		
		Student [] array = new Student[2]; // 배열의 크기는 한번 정하면 수정 불가
		
		Scanner sc = new Scanner(System.in);
		
		for(int i = 0; i < 2; i++) {
			System.out.print("학번 : ");
			String no = sc.next();
			System.out.print("국어 : ");
			int kor = sc.nextInt();
			System.out.print("영어 : ");
			int eng = sc.nextInt();
			System.out.print("수학 : ");
			int math = sc.nextInt();
			
			array[i].no = no;
		}
		
	}

}
